{
    "name": "puritandesigns/poindexter",
    "description": "A formula-calculating library",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "puritandesigns@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Poindexter\\": "src/Poindexter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/Tests/"
        }
    }
}
